public class stars
{
    public static void main(String[] args)
    {
        String star = "*";

        for (int i = 0; i <= 10; i++)
        {
            for (int j = i; j <= 10; j++) {
                    System.out.print(star);
                }// end of inner for loop
                System.out.println();
        }//end of for loop

    }// end of main method
}// end of class stars
