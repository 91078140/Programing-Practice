import java.lang.Math;
import java.util.*;

public class Coordinates {
    public static void main(String args[]) {
       Scanner keyboard = new Scanner(System.in);
       int x1, y1, x2, y2;
       double distance, xy;
       
       
       System.out.println("Enter two coordinates.");
       x1 = keyboard.nextInt();
       y1 = keyboard.nextInt();
       x2 = keyboard.nextInt();
       y2 = keyboard.nextInt();
       
       xy = Math.pow((x2 - x1), 2) + Math.pow((y2 - y1), 2);
       distance = Math.sqrt(xy);

      System.out.println("The distance between them is " + distance + ".");
    }// end of main method
}// end of class
